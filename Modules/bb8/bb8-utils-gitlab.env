    local _PullRequestUuid="$(_t4dGitLabUtilsGetPullRequestUuid $_Url)"
    _t4dGitLabMergeRequestAPIGet $(echo $_PullRequestUuid | sed 's/|/ /g') "${_OuputjsonFile}"
