###
###### DOCUMENTATION BLOC #####
########### CODE BLOC #########

    _t4dDebugLog $perror "Paint hasn't dried yet, please come back later"
    return 1

    local _t4dErrorCode=1
    local _Url=""
    local _Args=""
    local _Branch=""
    local _NewTargetBranch=""
    local _OuputjsonFile="$T4D_PROJECT_LOG_PATH/$T4D_BB8_MODE/_t4dModBb8DuplicatePullRequest.json"

    while [[ "$@" != "" ]]; do
        case "$1" in
            -url)
                _Url="$2"
                shift
                shift
                
            ;;
