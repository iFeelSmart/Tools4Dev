                shift
                shift
            ;;
            -tag|-t)
                _TAG="$2"
                shift
                shift
            ;;
            -order|-o)
                if [[ "$2" == "asc" ]] || [[ "$2" == "desc" ]]; then
                    _ORDER="&sort=$2"
                    shift
                    shift
                else
                    _t4dModBb8UtilsError 321 "_t4dModBb8UtilsRepoList" "ORDER $2" && return 321
                fi
            ;;
            -limit)
                if [[ "$(expr $(echo $2) + 0)" == "$2" ]]; then
                    _LIMIT="$2"
                    shift
                    shift
                else
                    _t4dModBb8UtilsError 321 "_t4dModBb8UtilsRepoList" "int for LIMIT $2" && return 321
                fi
            ;;
            -vvv)
                shift
                _verbosityLevel="3"
            ;;
            -vv)
                shift
                _verbosityLevel="2"
            ;;
            -v)
                shift
                _verbosityLevel="1"
            ;;
