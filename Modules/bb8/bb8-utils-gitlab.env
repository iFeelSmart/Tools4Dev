    _t4dSrcGitUtilsDuplicateBranch -b $_Branch -start-from "$__TARGET_BRANCH" -cb "${_Branch}-backported-to-$(echo $_NewTargetBranch | sed 's|/|-|g')" -target "$_NewTargetBranch" -m "Backport of $_Url" $(echo $_Args)
