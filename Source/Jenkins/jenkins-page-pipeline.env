#       
###
###### DOCUMENTATION BLOC #####
#EOF
########## CODE BLOC ##########

    local _BRANCH="$(echo $2 | sed 's|/|%2F|g')"
    cat $1 | grep -Eo "<tr id=\"job_$_BRANCH\".{0,100}" | cut -d '>' -f1 | grep -Eo 'class=.{0,100}' | cut -d '"' -f2 | sed 's| ||g'

###############################
_t4dDebugInfoEnd "_t4dSrcJenkinsPagePipelineGetBranchStatus" "$@"
}
