# v1.8.0
###### _t4dSrcMatrixMessageParsing
#   - Name:
#       _t4dSrcMatrixMessageParsing
###
#   - Synopsis:
#       description
###
#   - Example:
#       * _t4dSrcMatrixMessageParsing "Title" "https://i.imgur.com/DBveMrC.png" "Build #54589 Failed" "* failed1" "7d5099f" "https://google.com"
###
#   - Note:
#       <Required>
#       [Optionnal]
#       {XXXXXXX}       = Value To Be Changed, if {URL} then replace {URL} with a valid URL
###
###### DOCUMENTATION BLOC #####
########### CODE BLOC #########
    local _t4dErrorCode=1
    local _Title="${1:-Title}"
    local _DefaultImg="https://i.imgur.com/DBveMrC.png"
    local _Img="${2:-$_DefaultImg}"
    local _DefaultStatus="Build #54589 Failed"
    local _Status="${3:-$_DefaultStatus}"
    local _DefaultLog="* failed1"
    local _log="$(echo ${4:-$_DefaultLog} | sed 's|\n|<br>|g')"
    local _Sha="${5:-7d5099f}"
    local _DefaultLink="https://google.com"
    local _Link="${6:-$_DefaultLink}"

cat >&1 << EOF

<h2>
    <font color='grey'><b>$_Title</b></font>
</h2>
<table cellspacing='0' cellpadding='0' border='0' bgcolor='#000000'>
    <tr>
        <td>
            <img src=$_Img align='center' width='64' height='64'>
        </td>
        <td>
        
            <a href='$_Link'><strong style='font-size: 65px;'>$_Status</strong></a>
        </td>
    </tr>
    <tr>
        <td>Logs</td>
        <td>$_log</td>
    <tr>
        <td>Changes</td>
        <td>$_Sha</td>
</table>

EOF
    

###############################
}


