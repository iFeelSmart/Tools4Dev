_t4dSrcMatrixNotif(){
# Generated From $Tools4Dev_PATH/Templates/function.env
# v1.5.0
###### _t4dSrcMatrixNotif
#   - Name:
#       _t4dSrcMatrixNotif
###
#   - Synopsis:
#       Send Notif threw slack workspace
###
#   - Example:
#       _t4dSrcMatrixNotif "$(_t4dSrcMatrixDefaultMessage)" "$WS_MATRIX_BASE_URL/{CUSTOM_CHANNEL_TOKEN}"
###
###### DOCUMENTATION BLOC #####
    local _Message="$1"
    local _ChannelUrl="$2"
    local _OutputJsonFile="$T4D_PROJECT_LOG_PATH/_t4dSrcMatrixNotif.json"
    curl -s -X POST --header "Content-Type: application/json" --data "$_Message" "${_ChannelUrl}" 2> /dev/null | _t4dConvertToJson $_OutputJsonFile
    local _result="$(_t4dGetJsonData $_OutputJsonFile ".success")"

    case $_result in 
        true)
            _t4dDebugLog $psucceed "Notification Sent"
        ;;
        *)
            _t4dDebugLog $perror "$(_t4dGetJsonData $_OutputJsonFile ".error")"
        ;;
    esac

###############################
}

