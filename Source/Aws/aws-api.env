_t4dSrcAwsCp(){
    if [[ "$JENKINS_HOME" != "" ]]; then
        $WS_AWS_CLI_PATH s3 cp $@ &> .AWStmp
        rm .AWStmp
    else
        $WS_AWS_CLI_PATH s3 cp $@
    fi
}

_t4dSrcAwsRm(){
    if [[ "$JENKINS_HOME" != "" ]]; then
        $WS_AWS_CLI_PATH s3 rm $@ &> .AWStmp
        rm .AWStmp
    else
        $WS_AWS_CLI_PATH s3 rm $@
    fi
}




