                            echo "     ${pugreen}Project Aliases${pstd}"
                            for _Alias in $(echo $_Aliases); do
                                local _AliasName=$(_t4dDocGetAliasContent $_Alias name)
                                local _AliasContent="$(_t4dDocGetAliasContent $_Alias content)"
