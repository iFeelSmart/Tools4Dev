                                            | sed "s|%$|\\__|g" \
                                            | sed "s|% |\\__ |g")"
    fi

    if [[ "$_SectionDoc" == "" ]]; then
        if [[ "$_AllowDocEmpty" == "-allow-empty" ]]; then
            echo ""
